{
  "scriptFile": "__init__.py",
  "entryPoint": "handle_graphql_request",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "request",
      "methods": ["get", "post"],
      "route": "graphql"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}
